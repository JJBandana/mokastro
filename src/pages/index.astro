import ScriptMaker from "../components/ScriptMaker.astro";
import '../styles/reset.css'

export const prerender = true
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
